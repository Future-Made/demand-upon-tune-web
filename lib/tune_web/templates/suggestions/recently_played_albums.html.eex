<article class="top-albums">
  <div class="filter">
    <div class="meta">
      <%= content_tag :h1, gettext("Recently played albums") %>
      <%= content_tag :p, gettext("Generated from recently played tracks.") %>
    </div>
  </div>
  <div class="results">
    <%= render_many @albums, TuneWeb.SearchView, "result.html", as: :item, socket: @socket %>
  </div>
</article>
